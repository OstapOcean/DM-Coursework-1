<Window x:Class="DMCP_Part_1.OutputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:graphx="clr-namespace:GraphX.Controls;assembly=GraphX.WPF.Controls"
        xmlns:local="clr-namespace:DMCP_Part_1"
        Title="OutputWindow" Height="400.8" Width="831">

    <Grid HorizontalAlignment="Left" Margin="19,7,0,0" Width="797" Height="361" VerticalAlignment="Top">
        <Grid HorizontalAlignment="Left" Height="283" Margin="10,10,0,0" VerticalAlignment="Top" Width="66">
            <Button Content="Next Step" HorizontalAlignment="Left" Margin="0,47,0,0" VerticalAlignment="Top" Width="66" Click="Button_Click_NextGraph"/>
            <Button Content="Prev Step" HorizontalAlignment="Left" Margin="0,74,0,0" VerticalAlignment="Top" Width="66" Click="Button_Click_PrevGraph" RenderTransformOrigin="0.561,1.182"/>
            <Button Content="Result" Margin="0,10,0,0" Click="Button_Click_ResultGraph" Height="24" VerticalAlignment="Top"/>
          

        </Grid>
        <Grid Margin="76,9,343,39">
            <graphx:ZoomControl x:Name="zoomLeft" Margin="0,0,0,-12" RenderTransformOrigin="0.383,0.396" HorizontalAlignment="Left" Width="378" Height="325" VerticalAlignment="Bottom">
                <local:GArea x:Name="FlowGraphArea" Margin="0,0,67,-31" RenderTransformOrigin="1.095,0.998">
                </local:GArea>
            </graphx:ZoomControl>

        </Grid>
        <Grid HorizontalAlignment="Left" Height="313" Margin="459,9,0,0" VerticalAlignment="Top" Width="328">
            <graphx:ZoomControl x:Name="zoomRight" Margin="0,0,0,-12" RenderTransformOrigin="0.383,0.396" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="328">
                <local:GArea x:Name="IncrementalGraphArea" Margin="0,0,67,-31" RenderTransformOrigin="1.095,0.998">
                </local:GArea>
            </graphx:ZoomControl>
        </Grid>

    </Grid>
</Window>
